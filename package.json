{
  "name": "awesome-react-native-base",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "@types/react": "^16.0.38",
    "@types/react-native": "^0.52.0",
    "@types/react-test-renderer": "^16.0.0",
    "jest-expo": "^26.0.0",
    "react-native-scripts-ts": "1.11.3",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "16.2.0",
    "ts-jest": "^22.4.2",
    "tslib": "^1.7.1",
    "typescript": "^2.7.0"
  },
  "main": "./node_modules/react-native-scripts-ts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts-ts start",
    "eject": "react-native-scripts-ts eject",
    "android": "react-native-scripts-ts android",
    "ios": "react-native-scripts-ts ios",
    "test": "node node_modules/jest/bin/jest.js",
    "coverage": "node node_modules/jest/bin/jest.js --coverage",
    "test:dev": "node node_modules/jest/bin/jest.js --watch"
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "@types/expo": "^25.0.2",
    "@types/react-navigation": "^1.2.3",
    "babel-jest": "^22.4.3",
    "babel-preset-react-native": "^4.0.0",
    "expo": "^25.0.0",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "native-base": "^2.4.1",
    "react": "16.2.0",
    "react-native": "0.52.0",
    "react-navigation": "^1.5.8"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "verbose": true,
    "transformIgnorePatterns": [
      "node_modules/(?!(.*)/)"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    }
  }
}
